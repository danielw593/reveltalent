'use strict'

angular.module('app.data', [])

.factory('dataStorage', ->

    return {
        getCompanies: ->
            names = window.data.csv.shift().split("\t")
            data = []
            for  line in window.data.csv
              values = line.split("\t")
              record = {}
              position = 0
              for name in names
                record[name] = values[position++]
              data.push record
            {names:names, companies:data}
    }
)


